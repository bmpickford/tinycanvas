{
    "env": {
        "browser": true,
        "es2021": true,
        "mocha": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:jsdoc/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "mocha",
        "jsdoc"
    ],
    "rules": {
        "valid-jsdoc": "off"
    },
    "settings": {
        "jsdoc": {
            "mode": "typescript"
        }
    }
}
