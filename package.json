{
  "name": "game",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "private": true,
  "type": "module",
  "files": [
    "dist/*"
  ],
  "size-limit": [
    {
      "path": "dist/**/*.(js)",
      "limit": "10 kB"
    }
  ],
  "scripts": {
    "start": "web-dev-server --node-resolve --open --watch --esbuild-target auto --app-index example/index.html",
    "build": "rollup -c rollup.config.js && npm run zip",
    "test": "web-test-runner test/**/*.test.js --node-resolve",
    "test:watch": "web-test-runner \"test/**/*.test.js\" --node-resolve --watch",
    "size": "npm run build && size-limit",
    "lint": "eslint lib/",
    "prepare": "husky install",
    "zip": "zip dist/index.zip dist/* -9dv",
    "doc": "jsdoc -c jsdoc.conf.json"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@size-limit/preset-small-lib": "^7.0.3",
    "@web/dev-server": "^0.1.28",
    "@web/dev-server-esbuild": "^0.2.16",
    "@web/rollup-plugin-html": "^1.10.1",
    "@web/test-runner": "^0.13.22",
    "@web/test-runner-commands": "^0.5.13",
    "@web/test-runner-junit-reporter": "^0.4.9",
    "@web/test-runner-playwright": "^0.8.8",
    "eslint": "^8.3.0",
    "eslint-plugin-jsdoc": "^37.1.0",
    "eslint-plugin-mocha": "^9.0.0",
    "husky": "^7.0.4",
    "jsdoc": "^3.6.7",
    "jsdoc-tsimport-plugin": "^1.0.5",
    "rollup": "^2.60.1",
    "rollup-plugin-bundle-summary": "^1.0.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "sinon": "^12.0.1",
    "size-limit": "^7.0.3"
  }
}
