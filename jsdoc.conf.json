{
    "plugins": [
        "node_modules/jsdoc-tsimport-plugin/index.js"
    ],
    "recurseDepth": 10,
    "sourceType": "module",
    "source": {
        "include": ["./packages"],
        "includePattern": "index.js"
    },
    "opts": {
        "destination": "./docs/",
        "recurse": "true"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}